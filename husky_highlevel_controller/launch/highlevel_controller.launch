<?xml version="1.0"?>
<launch>
    <rosparam file="$(find husky_highlevel_controller)/config/params.yaml" />

    <node name="laser_scan" pkg="husky_highlevel_controller" type="controller_node.py" output="screen"/>
    <node name="husky_target_drive" pkg="husky_highlevel_controller" type="target_drive_node.py" output="screen"/>
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find husky_highlevel_controller)/config/config.rviz"/>

    <include file="$(find husky_gazebo)/launch/husky_empty_world.launch">
        <arg name="world_name" value="worlds/03_single_pillar.world"/>
    </include>

</launch>